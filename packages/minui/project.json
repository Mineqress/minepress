{
  "name": "minui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/minui/src",
  "targets": {
    "build": {
      "executor": "@monodon/rust:napi",
      "options": {
        "dist": "dist/packages/minui",
        "jsFile": "index.js"
      },
      "configurations": {
        "development": {
          
        },
        "production": {
          "release": true
        }
      }
    },
    "test": {
      "executor": "@monodon/rust:test",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/minui"
      },
      "configurations": {
        "development": {},
        "production": {
          "release": true
        }
      }
    },
    "lint": {
      "executor": "@monodon/rust:lint",
      "outputs": ["{options.target-dir}"],
      "options": {
        "target-dir": "dist/target/minui"
      },
      "configurations": {
        "development": {}
      }
    }
  },
  "tags": []
}
